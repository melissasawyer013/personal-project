<!-- Navbar -->
<%- include('../partials/header')-%>
<!-- Page Content Starts -->
    <div class="page-container-column">
<!-- Form Starts Here -->
        <div class="large-content-container-column palette-b">
            <h1>PROFILE</h1>
            <% if(typeof messages != "undefined") { %>
                <div class="alert-messages palette-d">
                    <span><%= messages[0] %></span>
                </div> 
             <%}%>
            <p>Hello, <%=user.formFirstName%>! <br>You can update your form entries below.</p>
            <form action="/logout?_method=DELETE" method="POST">
                <input type="submit" class="palette-a button" value="Logout">
                <br>
            </form>
            <div class="form-container">
                <form action="/updateFormData" method="POST">
                    <h2>UPDATE THE FORM BELOW</h2>
                    <p>You can update your answers in the form below. Fields marked with an asterisk * are required. You will be able to choose which information you want to display publicly.</p>
                    <br>
                    <hr>
<!-- Name Section Starts Here -->
                    <fieldset>
                        <legend class="header-legend">NAME</legend>
                        <div class="input-containers">
                            <label for="formFirstName" class="form-labels">First Name *</label>
                            <input type="text" placeholder="first name" value="<%= user.formFirstName %>" id="formFirstName"
                            name="formFirstName" class="text-inputs" required>
                        </div>
                        <div class="input-containers">
                            <label for="formLastName" class="form-labels">Last Name *</label>
                            <input type="text" placeholder="last name" value="<%= user.formLastName %>" id="formLastName" 
                            name="formLastName" class="text-inputs" required>
                        </div>
                        <br>
                        <fieldset>
                            <legend class="sub-field-legend">What information can we share publicly? *</legend>
                            <span class="form-labels-subtext">(choose one)</span>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formFullNamePublic" id="formFullNamePublic" name="formPublicDisplayName" value="full name" required <% if(user.formPublicDisplayName === 'formFullNamePublic') { %> checked <%} %> >
                                <label for="formFullNamePublic" class="form-labels-check-radio">full name</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" id="firstNameOnlyPublic" value="formFirstNameOnlyPublic" name="formPublicDisplayName" value="first name only" required <% if(user.formPublicDisplayName === 'formFirstNameOnlyPublic') { %> checked <%} %> >
                                <label for="formFirstNameOnlyPublic" class="form-labels-check-radio">first name only</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formLastNameOnlyPublic" id="formLastNameOnlyPublic" name="formPublicDisplayName" value="last name only" required <% if(user.formPublicDisplayName === 'formLastNameOnlyPublic') { %> checked <%} %> >
                                <label for="formLastNameOnlyPublic" class="form-labels-check-radio">last name only</label> 
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formPseudonymPublic" id="formPseudonymPublic" name="formPublicDisplayName" value="pseudonym" required <% if(user.formPublicDisplayName === 'formPseudonymPublic') { %> checked <%} %> >
                                <label for="formPseudonymPublic" class="form-labels-check-radio">pseudonym (generate a fake name)</label>
                            </div>
                        </fieldset>
                    </fieldset>
                    <br>
                    <hr>
<!-- Contact Information Section Starts Here -->
                    <fieldset>
                        <legend class="header-legend">CONTACT INFORMATION</legend>
                        <div class="input-containers">
                            <label for="formPhoneNumber" class="form-labels">Phone Number *</label>
                            <input type="tel" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}" placeholder="012-345-6789" value="<%= user.formPhoneNumber %>" id="formPhoneNumber" class="text-inputs" name="formPhoneNumber" required>
                        </div>
                        <div class="input-containers">
                            <label for="formEmail" class="form-labels">Email Address *</label>
                            <input type="email" placeholder="YourEmail@email.com" value="<%= user.formEmail %>" id="formEmail" class="text-inputs" name="formEmail" required>
                        </div>
                        <br>
                        <fieldset>
                            <legend class="sub-field-legend">What information can we share publicly? *</legend>
                            <span class="form-labels-subtext">(choose one)</span>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formPhoneAndEmailPublic" id="formPhoneAndEmailPublic" name="formPublicDisplayContact" required <% if(user.formPublicDisplayContact === 'formPhoneAndEmailPublic') { %> checked <%} %> >
                                <label for="formPhoneAndEmailPublic" class="form-labels-check-radio">phone number and email</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formPhoneNumberOnlyPublic" id="formPhoneNumberOnlyPublic" name="formPublicDisplayContact" required <% if(user.formPublicDisplayContact === 'formPhoneNumberOnlyPublic') { %> checked <%} %> >
                                <label for="formPhoneNumberOnlyPublic" class="form-labels-check-radio">phone number only</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formEmailOnlyPublic" id="formEmailOnlyPublic" name="formPublicDisplayContact" required <% if(user.formPublicDisplayContact === 'formEmailOnlyPublic') { %> checked <%} %> >
                                <label for="formEmailOnlyPublic" class="form-labels-check-radio">email address only</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formNothingPublic" id="formNothingPublic" name="formPublicDisplayContact" required <% if(user.formPublicDisplayContact === 'formNothingPublic') { %> checked <%} %> >
                                <label for="formNothingPublic" class="form-labels-check-radio">neither - don't display any contact information publicly</label>
                            </div>
                        </fieldset>
                    </fieldset>
                    <br>
                    <hr>
<!-- Address Information Section Starts Here-->
                    <fieldset>
                        <legend class="header-legend">LOCATION</legend>
                        <div class="input-containers">
                            <label for="formAddressHouseNumber" class="form-labels">House Number</label>
                            <input type="text" placeholder="12345" value="<%= user.formAddressHouseNumber %>" id="formAddressHouseNumber" class="address-number-inputs" name="formAddressHouseNumber">
                        </div>
                        <div class="input-containers">
                            <label for="formAddressStreetName" class="form-labels">Street Name *</label>
                            <input type="text" placeholder="Dorchester Avenue" value="<%= user.formAddressStreetName %>" id="formAddressStreetName" class="text-inputs" name="formAddressStreetName" required>
                        </div>
                        <div class="input-containers">
                            <label for="formAddressApartmentNumber" class="form-labels">Apartment/Unit Number <span class="form-labels-subtext">(optional)</span></label>
                            <input type="text" placeholder="Unit 3" value="<%= user.formAddressApartmentNumber %>" id="formAddressApartmentNumber" class="address-number-inputs" name="formAddressApartmentNumber">
                        </div>
                        <br>
                        <fieldset>
                            <legend class="sub-field-legend">What sub-neighborhood of Dorchester do you live in? *</legend>
                            <span class="form-labels-subtext">(choose one)</span>
                            <div class="input-container-check-radio">
                                <input type="radio" value="JFK / Southie / Savin" id="formNeighborhoodJfkSouthieSavin" name="formNeighborhood" required <% if(user.formNeighborhood === 'JFK / Southie / Savin') { %> checked <%} %> >
                                <label for="formNeighborhoodJfkSouthieSavin" class="form-labels-check-radio">JFK/Southie/Savin</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Grove Hall" id="formNeighborhoodGroveHall" name="formNeighborhood" required <% if(user.formNeighborhood === 'Grove Hall') { %> checked <%} %> >
                                <label for="formNeighborhoodGroveHall" class="form-labels-check-radio">Grove Hall</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Uphams" id="formNeighborhoodUphams" name="formNeighborhood" required <% if(user.formNeighborhood === 'Uphams') { %> checked <%} %> >
                                <label for="formNeighborhoodUphams" class="form-labels-check-radio">Uphams</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Bowdoin / Geneva" id="formNeighborhoodBowdoinGeneva" name="formNeighborhood" required <% if(user.formNeighborhood === 'Bowdoin / Geneva') { %> checked <%} %> >
                                <label for="formNeighborhoodBowdoinGeneva" class="form-labels-check-radio">Bowdoin/Geneva</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Fields Corner" id="formNeighborhoodFieldsCorner" name="formNeighborhood" required <% if(user.formNeighborhood === 'Fields Corner') { %> checked <%} %> >
                                <label for="formNeighborhoodFieldsCorner" class="form-labels-check-radio">Fields Corner</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="St. Marks / Neponset" id="formNeighborhoodStMarksNeponset" name="formNeighborhood" required <% if(user.formNeighborhood === 'St. Marks / Neponset') { %> checked <%} %> >
                                <label for="formNeighborhoodStMarksNeponset" class="form-labels-check-radio">St. Marks/Neponset</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Codman" id="formNeighborhoodCodman" name="formNeighborhood" required <% if(user.formNeighborhood === 'Codman') { %> checked <%} %> >
                                <label for="formNeighborhoodCodman" class="form-labels-check-radio">Codman</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Ashmont" id="formNeighborhoodAshmont" name="formNeighborhood" required <% if(user.formNeighborhood === 'Ashmont') { %> checked <%} %> >
                                <label for="formNeighborhoodAshmont" class="form-labels-check-radio">Ashmont</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Blue Hill" id="formNeighborhoodBlueHill" name="formNeighborhood" required <% if(user.formNeighborhood === 'Blue Hill') { %> checked <%} %> >
                                <label for="formNeighborhoodBlueHill" class="form-labels-check-radio">Blue Hill</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Lower Mills" id="formNeighborhoodLowerMills" name="formNeighborhood" required <% if(user.formNeighborhood === 'Lower Mills') { %> checked <%} %> >
                                <label for="formNeighborhoodLowerMills" class="form-labels-check-radio">Lower Mills</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Mattapan" id="formNeighborhoodMattapan" name="formNeighborhood" required <% if(user.formNeighborhood === 'Mattapan') { %> checked <%} %> >
                                <label for="formNeighborhoodMattapan" class="form-labels-check-radio">Mattapan</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="Unknown / Not Dorchester" id="formNeighborhoodUnknown" name="formNeighborhood" required <% if(user.formNeighborhood === 'Unknown / Not Dorchester') { %> checked <%} %> >
                                <label for="formNeighborhoodUnknown" class="form-labels-check-radio">Don't know / Don't live in Dorchester or Mattapan'</label>
                            </div>
                        </fieldset>
                        <br>
                        <fieldset>
                            <legend class="sub-field-legend">What information can we share publicly? *</legend>
                            <span class="form-labels-subtext">(choose one)</span>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formFullAddressPublic" id="formFullAddressPublic" name="formPublicDisplayAddress" required <% if(user.formPublicDisplayAddress === 'formFullAddressPublic') { %> checked <%} %> >
                                <label for="formFullAddressPublic" class="form-labels-check-radio">full address + neighborhood</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formStreetNameOnlyPublic" id="formStreetNameOnlyPublic" name="formPublicDisplayAddress" required <% if(user.formPublicDisplayAddress === 'formStreetNameOnlyPublic') { %> checked <%} %> >
                                <label for="formStreetNameOnlyPublic" class="form-labels-check-radio">street name only + neighborhood</label>
                            </div>
                            <div class="input-container-check-radio">
                                <input type="radio" value="formNeighborhoodOnlyPublic" id="formNeighborhoodOnlyPublic" name="formPublicDisplayAddress" required <% if(user.formPublicDisplayAddress === 'formNeighborhoodOnlyPublic') { %> checked <%} %> >
                                <label for="formNeighborhoodOnlyPublic" class="form-labels-check-radio">neighborhood only</label>
                            </div>
                        </fieldset>
                    </fieldset>
                    <br>
                    <hr>
<!-- Offerings Section Starts Here-->
                    <fieldset class="checkForChecks">
                        <legend class="header-legend">OFFERINGS</legend>
                        <fieldset>
                            <legend class="sub-field-legend">What can you offer or share with your neighbors?</legend>
                            <span class="form-labels-subtext">This information will be viewable on the Needs and Offerings page along with the personal information you've chosen to share. (check all that apply)</span>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Childcare" id="formOfferChildcare" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Childcare') || ((user.formOfferings).includes("Offer: Childcare"))) { %> checked<% }} %> >
                                    <label for="formOfferChildcare" class="form-labels-check-radio">childcare</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your childcare offering so people can directly reach out to you.</legend>
                                        <label for="explainOfferChildcare" class="form-labels-subtext">Examples: What experience do you have with childcare? Would you be able to go to the child's house or will they be required to come to yours? What precautions are you taking around the COVID-19 virus? What is your availability?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferChildcare" name="explainOfferChildcare"><%= user.explainOfferChildcare %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Cooking" id="formOfferCooking" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Cooking') || ((user.formOfferings).includes('Offer: Cooking'))) { %>checked<% }} %> >
                                    <label for="formOfferCooking" class="form-labels-check-radio">cooking for someone</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering to cook for someone so people can directly reach out to you.</legend>
                                        <label for="explainOfferCooking" class="form-labels-subtext">Examples: How often could you cook for someone? What types of foods can you cook? Can you accomodate allergies and dietary restrictions? How would you transport the food to your neighbor? What precautions are you taking around the COVID-19 virus?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferCooking" name="explainOfferCooking"><%= user.explainOfferCooking %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Food and Supplies" id="formOfferFoodAndSupplies" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Food and Supplies') || ((user.formOfferings).includes('Offer: Food and Supplies'))) { %>checked<% }} %> >
                                    <label for="formOfferFoodAndSupplies" class="form-labels-check-radio">food & supplies</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of food and supplies so people can directly reach out to you.</legend>
                                        <label for="explainOfferFoodAndSupplies" class="form-labels-subtext">Examples: What kind of food and/or supplies do you have to offer? How often will you have extra supplies and/or food available? What precautions are you taking around the COVID-19 virus? How would you distribute extra food and/or supplies?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferFoodAndSupplies" name="explainOfferFoodAndSupplies"><%= user.explainOfferFoodAndSupplies %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Rides or Access to a Vehicle" id="formOfferRidesCar" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Rides or Access to a Vehicle') || ((user.formOfferings).includes('Offer: Rides or Access to a Vehicle'))) { %>checked<% }} %> >
                                    <label for="formOfferRidesCar" class="form-labels-check-radio">give rides or access to a vehicle</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of rides and/or vehicle access so people can directly reach out to you.</legend>
                                        <label for="explainOfferRidesCar" class="form-labels-subtext">Examples: What is your availability for giving rides if that is what you are offering? What is the vehicle availability if that is what you are offering? What precautions are you taking around the COVID-19 virus? What precautions would you need neighbors to follow for this offering?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferRidesCar" name="explainOfferRidesCar"><%= user.explainOfferRidesCar %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: PetCare / Dog-Walking" id="formOfferPetCare" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: PetCare / Dog-Walking') || ((user.formOfferings).includes('Offer: Offer: PetCare / Dog-Walking'))) { %>checked<% }} %> >
                                    <label for="formOfferPetCare" class="form-labels-check-radio">petcare/dog-walking</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of petcare/dog-walking so people can directly reach out to you.</legend>
                                        <label for="explainOfferPetCare" class="form-labels-subtext">Examples: Explain what you are offering. What is your availability for petcare and/or dog-walking? What precautions are you taking around the COVID-19 virus? What precautions would you need your neighbors to follow for this offering?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferPetCare" name="explainOfferPetCare"><%= user.explainOfferPetCare %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Internet Subscriptions" id="formOfferInternetSubs" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Internet Subscriptions') || ((user.formOfferings).includes('Offer: Internet Subscriptions'))) { %>checked<% }} %> >
                                    <label for="formOfferInternetSubs" class="form-labels-check-radio">internet subscriptions (share your Netflix, Hulu, Prime, HBO, etc.)</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of internet subscriptions so people can directly reach out to you.</legend>
                                        <label for="explainOfferInternetSubs" class="form-labels-subtext">Examples: What subscriptions are you offering to share? How can neighbors contact you to exchange login information?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferInternetSubs" name="explainOfferInternetSubs"><%= user.explainOfferInternetSubs %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Medical Support" id="formOfferMedicalSupport" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Medical Support') || ((user.formOfferings).includes('Offer: Medical Support'))) { %>checked<% }} %> >
                                    <label for="formOfferMedicalSupport" class="form-labels-check-radio">medical support (pick up prescriptions, help with hotlines, etc.)</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of medical support so people can directly reach out to you.</legend>
                                        <label for="explainOfferMedicalSupport" class="form-labels-subtext">Examples: What type of support are you offering? If you're offering to pick up prescriptions, how often could you do so? What precautions are you taking around the COVID-19 virus? What precautions would you need your neighbors to follow for this offering?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferMedicalSupport" name="explainOfferMedicalSupport"><%= user.explainOfferMedicalSupport %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Medical Advice" id="formOfferMedicalAdvice" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Medical Advice') || ((user.formOfferings).includes('Offer: Medical Advice'))) { %>checked<% }} %> >
                                    <label for="formOfferMedicalAdvice" class="form-labels-check-radio">medical advice (if you are a medical professional only, please)</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of medical advice so people can directly reach out to you.</legend>
                                        <label for="explainOfferMedicalAdvice" class="form-labels-subtext">Examples: Explain the nature of your offering. What type of advice are you qualified to offer? What are your credentials? How can neighbors contact you about this offering?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferMedicalAdvice" name="explainOfferMedicalAdvice"><%= user.explainOfferMedicalAdvice %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Storage Space" id="formOfferStorage" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Storage Space') || ((user.formOfferings).includes('Offer: Storage Spacee'))) { %>checked<% }} %> >
                                    <label for="formOfferStorage" class="form-labels-check-radio">storage space</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of storage space so people can directly reach out to you.</legend>
                                        <label for="explainOfferStorage" class="form-labels-subtext">Examples: How much space do you have available? Explain the accessibility of the space. What precautions are you taking around the COVID-19 virus? What precautions would you need your neighbors to follow for this offering?</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferStorage" name="explainOfferStorage"><%= user.explainOfferStorage %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
                                <div class="input-container-check-radio">
                                    <input type="checkbox" value="Offer: Art / Music / Writing" id="formOfferArt" class="contains-hidden-div-update" name="formOfferings" <% if(user.formOfferings) {if ((user.formOfferings === 'Offer: Art / Music / Writing') || ((user.formOfferings).includes('Offer: Art / Music / Writing'))) { %>checked<% }} %> >
                                    <label for="formOfferArt" class="form-labels-check-radio">art, music, writing</label>
                                </div>
                                <fieldset class="initially-display-none-explain-more-container">
                                    <div class="input-containers">
                                        <legend class="sub-field-legend">Please elaborate on your offering of art, music, or writing so people can directly reach out to you.</legend>
                                        <label for="explainOfferArt" class="form-labels-subtext">Examples: Explain what you are offering. What kind of art do you do? How would you get this art to your neighbors? How can neighbors contact you to request art, music, and/or writing.</label>
                                        <textarea class="text-inputs" placeholder="Explain here" id="explainOfferArt" name="explainOfferArt"><%= user.explainOfferArt %></textarea>
                                    </div>
                                    <br>
                                </fieldset>
                            </div>
                            <div>
